lBC,KAAK,CAAE,4BADW,CAElBC,EAAE,CAAE,4BAFc,CAGlBC,QAAQ,CAAE,EAHQ,CAAD,CAIhB,CACDF,KAAK,CAAE,gBADN,CAEDC,EAAE,CAAE,gBAFH,CAGDC,QAAQ,CAAE,EAHT,CAJgB,CAAZ,CAUP,KAAMC,CAAAA,WAAW,CAAG,CAClBJ,GADkB,CAApB,CAGA,KAAMK,CAAAA,SAAS,CAAG,SAAlB,CACA,cAAe,SAASC,CAAAA,UAAT,CAAoB,CACjCC,UADiC,CAEjC,GAAGC,KAF8B,CAApB,CAGZ,CACD,MAAO,KAAC,SAAD,aAAeJ,WAAf,CAAgCI,KAAhC,EAAuC,UAAU,CAAED,UAAnD,CAA+D,OAAO,CAAC,WAAvE,GAEL,UACE,KAAM,4BADR,EAEK,4BAFL,CAFK,CAKL,aAAG,cAAQ,UAAU,CAAC,GAAnB,EAAyB,2EAAzB,CAAH,CALK,CAML,aAAK,sBAAL,CANK,CAOL,eAAK,YAAM,UAAU,CAAC,KAAjB,EAAiC;AAC1C,CADS,CAAL,CAPK,CASL,aAAK,+HAAL,CATK,CAUL,aAAK,+LAAL,CAVK,CAWL,eAAK,YAAM,UAAU,CAAC,KAAjB,EAAiC;AAC1C,CADS,CAAL,CAXK,CAaL,aAAK,wDAAL,CAbK,CAcL,aAAK,mDAAL,CAdK,CAeL,eAAK,YAAM,UAAU,CAAC,KAAjB,EAAiC;AAC1C,CADS,CAAL,CAfK,CAiBL,aAAK,4GAAL,CAjBK,CAkBL,UACE,KAAM,gBADR,EAEK,gBAFL,CAlBK,CAqBL,aAAK,wDAAL,CArBK,CAsBL,eAAK,YAAM,UAAU,CAAC,KAAjB,EAAiC;AAC1C,CADS,CAAL,CAtBK,CAwBL,aAAG,cAAQ,UAAU,CAAC,GAAnB,EAAyB,MAAzB,CAAH,CAA6C,6FAA7C,CAxBK,CAyBL,aAAK,yCAAL,CAzBK,CA0BL,eAAK,YAAM,UAAU,CAAC,KAAjB,EAAiC;AAC1C,CADS,CAAL,CA1BK,CA4BL,aAAK,iDAAL,CA5BK,CAAP,CA8BD,CAED,CACAD,UAAU,CAACG,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tTitle: 'Initialization'\n};\nexport const metadata = {\n  \"unversionedId\": \"Initialization\",\n  \"id\": \"Initialization\",\n  \"isDocsHomePage\": false,\n  \"title\": \"Initialization\",\n  \"description\": \"Manual Setup of a new Node\",\n  \"source\": \"@site/docs/Initialization.md\",\n  \"slug\": \"/Initialization\",\n  \"permalink\": \"/Initialization\",\n  \"editUrl\": \"https://github.com/IDEP-network/IDEP-Docs/docs/Initialization.md\",\n  \"version\": \"current\",\n  \"sidebar\": \"docs\",\n  \"previous\": {\n    \"title\": \"Installation\",\n    \"permalink\": \"/Installation\"\n  },\n  \"next\": {\n    \"title\": \"Joining IDEP's Public Testnet Denali\",\n    \"permalink\": \"/Denali\"\n  }\n};\n  /* @jsxRuntime classic */\n/* @jsx mdx */\n\nexport const toc = [{\n  value: 'Manual Setup of a new Node',\n  id: 'manual-setup-of-a-new-node',\n  children: []\n}, {\n  value: 'Start the node',\n  id: 'start-the-node',\n  children: []\n}];\n\nconst layoutProps = {\n  toc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"manual-setup-of-a-new-node\"\n    }}>{`Manual Setup of a new Node`}</h2>\n    <p><strong parentName=\"p\">{`These instructions are for setting up a brand new full node from scratch.`}</strong></p>\n    <p>{`Initialize the node:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`iond init <moniker> --chain-id Test-Denali\n`}</code></pre>\n    <p>{`Note Monikers can contain only ASCII characters. Using Unicode characters is not supported and renders your node unreachable.`}</p>\n    <p>{`By default, the init command creates your ~/.ion directory with subfolders config and data. In the config directory, the most important files for configuration are app.toml and config.toml.`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`iond keys add <accountname>\n`}</code></pre>\n    <p>{`Make sure you save your mnemonic in a secure location!`}</p>\n    <p>{`Next make your way to the nodes config directory:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`cd ~/.ion/config/\n`}</code></pre>\n    <p>{`Remove the genesis.json and replace it with the one provided for the network you are trying to connect to.`}</p>\n    <h2 {...{\n      \"id\": \"start-the-node\"\n    }}>{`Start the node`}</h2>\n    <p>{`You can now start the node with the following command:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`iond start --p2p.persistent_peers=3285f676050842aea5ce0c5968ef4b1579ffc8d9@104.131.46.133:26656,19f7aa0f2da7e6a01372be459a9c43bec6ac2191@64.225.75.108:26656\n`}</code></pre>\n    <p><strong parentName=\"p\">{`Note`}</strong>{` The following example is taken from the testnet Denali, and will be different for mainnet.`}</p>\n    <p>{`Now check the status of your node with:`}</p>\n    <pre><code parentName=\"pre\" {...{}}>{`iond status\n`}</code></pre>\n    <p>{`You can stop the node at any time using Ctrl+C.`}</p>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}